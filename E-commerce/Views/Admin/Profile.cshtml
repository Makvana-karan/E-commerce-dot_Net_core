@model  List<Admin>
<div class="container-fluid px-4">
    <h2 class="mt-2 text-center text-uppercase" style="letter-spacing:4px">My Profile</h2>
</div>

<div class="d-flex justify-content-center align-items-center p-4 ">
    <div class=" col-md-2"></div>
    <div class="col-md-8">
<form method="post">
    <input type="hidden" name="admin_id" value="@Model[0].admin_id" />
    <input type="type" name="name" class=" mb-2 form-control" placeholder="Name" value="@Model[0].admin_name" name="admin_name"/>
            <input type="email" name="name" class="mb-2 form-control" placeholder="Name" value="@Model[0].admin_email" name="admin_email" />
            <input type="password" name="name" class="mb-2 form-control" placeholder="Name" value="@Model[0].admin_password" name="admin_password" />
            <input type="hidden" name="admin_image" value="@Model[0].admin_image" />
    <input type="submit" value="uppdate" class="form-control my-3 btn btn-dark" />
</form>
    </div>
    <div class="col-md-2"></div>
</div>
